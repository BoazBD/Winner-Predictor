    "version": "0.2.0",
    "configurations": [
                {
            "name": "Scraper",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scraper.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "ENV": "local"
            }
        },
        {
            "name": "API Scraper",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scraper/main.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "ENV": "local",
                "PYTHONPATH": "${workspaceFolder}/scraper"
            }
        },
        {
            "name": "Gold ETL",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/gold_etl.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "ENV": "local",
                "PYTHONPATH": "${workspaceFolder}/scripts",
                "AWS_PROFILE": "default",
                "AWS_ACCESS_KEY_ID": "",
                "AWS_SECRET_ACCESS_KEY": "",
                "AWS_SESSION_TOKEN": ""
            }
        },
        {
            "name": "Results ETL",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/get_results.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "ENV": "local",
                "PYTHONPATH": "${workspaceFolder}/"
            }
        },
        {
            "name": "Train Model",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/train_model.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "ENV": "local",
                "COMPETITION_LEVEL": "big_games",
                "PYTHONPATH": "${workspaceFolder}/scripts",
                "MAX_SEQ_LENGTH": "12"

            }
        },
        {
            "name": "Evaluate Model",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/evaluate_model_roi.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "ENV": "local",
                "MODEL_TYPE": "lstm",
                "EPOCHS": "100",
                "PYTHONPATH": "${workspaceFolder}/scripts",
                "MAX_SEQ": "12",
                "THRESHOLD": "0.00",
                "MODEL_VERSION": "1"
            }
        },
        {
            "name": "evaluate_multiple_models",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/evaluate_lstms.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "ENV": "local",
                "MODEL_TYPE": "all_games",
                "MODEL" : "good_model.h5",
                "PYTHONPATH": "${workspaceFolder}/"
            }
        },
        {
            "name": "evaluate_transformers",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/evaluate_transformers.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "ENV": "local",
                "PYTHONPATH": "${workspaceFolder}/"
            }
        },
        {
            "name": "Data Processing - Standard",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/data_processing.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--input", "processed_winner_100725.parquet",
                "--max_seq_length", "12",
                "--scaling_method", "standard",
                "--competition_level", "big_games"
            ],
            "env": {
                "ENV": "local",
                "PYTHONPATH": "${workspaceFolder}/scripts"
            }
        },
        {
            "name": "Data Processing - ROI",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/data_processing.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--input", "processed_winner_100725.parquet",
                "--output", "processed_features_roi_18.pkl",
                "--max_seq_length", "18",
                "--scaling_method", "roi",
                "--competition_level", "big_games"
            ],
            "env": {
                "ENV": "local",
                "PYTHONPATH": "${workspaceFolder}/scripts"
            }
        },
        {
            "name": "Predict Model Firestore Lambda - Local",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/lambdas/predict_model_firestore/lambda_function.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${workspaceFolder}",
            "env": {
                "ENV": "local",
                "IS_LOCAL": "False",
                "AWS_REGION": "il-central-1",
                "ATHENA_DATABASE": "winner-db",
                "S3_BUCKET": "winner-site-data",
                "MODEL_TYPE": "lstm",
                "EPOCHS": "100",
                "MAX_SEQ": "12",
                "THRESHOLD": "0.00",
                // "PYTHONPATH": "${workspaceFolder}:${workspaceFolder}/lambdas/predict_model_firestore",
                "GOOGLE_APPLICATION_CREDENTIALS": "${workspaceFolder}/lambdas/predict_model_firestore/service-account.json"
            }
        }
    ]
}